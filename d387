#include<stdio.h>
#include<stdlib.h>
int main (){
    int n,i,j;

    while(scanf("%d",&n)==1){
        int a=1;
        if(n<2) a=0;
        for(i=2;i*i<=n;i++){   
            if(n%i==0) {
                a=0;
                break;    
            }
        }
        if(a==0){
            printf("%d is not prime.\n", n);
            continue;
        }
        int nice=0;
        j=n;
        while(j>0){
            nice=nice*10+j%10;
            j=j/10;
        }
        a=1;
        if(nice<2) a=0;
        for(i=2;i*i<=nice;i++){
            if(nice%i==0) {
                a=0;
                break;
            }
        }
        if(a==1 && nice!=n)
            printf("%d is emirp.\n",n);
        else
            printf("%d is prime.\n",n);
    }     
    return 0;
}

